extends layout

block content
  h1= title
  p Bienvenue sur le #{title}

  <form metohd="post" id="form">

    <div class="form-group">
      <label for="exampleName">Prénom</label>
      <input type="text" class="form-control" id="exampleInputName1" placeholder="Prénom">

      <label for="exampleName">Nom</label>
      <input type="text" class="form-control" id="exampleInputName2" placeholder="Nom">
    </div>
    <div class="form-group">
      <label for="exampleInputEmail1">Email</label>
      <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Email">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Numéro Portable</label>
      <input type="number" class="form-control" id="exampleInputPassword1" placeholder="Numéro Portable">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Numéro Fix</label>
      <input type="number" class="form-control" id="exampleInputPassword2" placeholder="Numéro Fix">
    </div>
    <button type="submit" class="btn btn-primary">Ajouter</button>
  
  </form>
